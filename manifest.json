{
  "manifest_version": 3,
  "name": "VTOP UI",
  "version": "1.0.5",
  "description": "Fixes grandpa VTOP.",
  "content_scripts": [
    {
      "matches": [
        "https://vtop.vit.ac.in/*"
      ],
      "js": [
        "src/content.js",
        "src/pdf-autoopen.js",
        "src/coursepage.js"
      ]
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "host_permissions": [
    "https://vtop.vit.ac.in/*"
  ],
  "permissions": [
    "alarms",
    "cookies",
    "scripting",
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/vtop-dock-inject.js",
        "src/vtop-loading.js",
        "src/spotlight-fix.js",
        "topbar-fix.js",
        "src/darkmode.js",
        "src/darkreader.min.js"
      ],
      "matches": [
        "https://vtop.vit.ac.in/*"
      ]
    }
  ],
  "icons": {
    "16": "src/logo.png",
    "32": "src/logo.png",
    "48": "src/logo.png",
    "128": "src/logo.png"
  },
  "action": {
    "default_popup": "src/popup2.html",
    "default_icon": {
      "16": "src/logo.png",
      "32": "src/logo.png",
      "48": "src/logo.png",
      "128": "src/logo.png"
    }
  }
}
